{
    "name": "jfcherng/php-aacodec",
    "description": "Encode/decode JavaScript to/from Japanese style emoticons.",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Izman",
            "email": "izmanw@gmail.com"
        },
        {
            "name": "Jack Cherng",
            "email": "jfcherng@gmail.com"
        }
    ],
    "keywords": [
        "codec",
        "encoder",
        "decoder",
        "aaencode",
        "aadecode",
        "aaencoder",
        "aadecoder",
        "javascript"
    ],
    "autoload": {
        "psr-4": {
            "Jfcherng\\AaCodec\\": "src/"
        }
    },
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phan/phan": "^1.0",
        "phpunit/phpunit": "^7.2 || ^8.0"
    },
    "scripts": {
        "analyze": [
            "phan --color"
        ],
        "fix": [
            "php-cs-fixer fix --verbose"
        ],
        "test": [
            "phpunit --verbose"
        ]
    }
}
